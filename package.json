{
  "name": "customer-shop",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "node scripts/validate-env.js && next dev",
    "build": "node scripts/validate-env.js && next build --webpack",
    "start": "next start",
    "validate-env": "node scripts/validate-env.js",
    "backup-env": "cp .env.local .env.backup",
    "restore-env": "cp .env.backup .env.local",
    "lint": "eslint",
    "lint:dev": "eslint --config eslint.config.dev.mjs",
    "lint:fix": "eslint --fix",
    "lint:fix:dev": "eslint --fix --config eslint.config.dev.mjs",
    "warm-cache": "node scripts/warm-deployment-cache.js",
    "cache:invalidate": "node scripts/invalidate-cache.js",
    "test": "playwright test tests/critical/",
    "test:ui": "playwright test tests/critical/ --ui",
    "test:headed": "playwright test tests/critical/ --headed",
    "test:debug": "playwright test tests/critical/ --debug",
    "test:critical": "playwright test tests/critical/",
    "test:security": "playwright test tests/security/",
    "test:e2e": "playwright test tests/e2e/",
    "test:mobile": "playwright test tests/mobile/",
    "test:mobile:chrome": "playwright test tests/mobile/ --project=\"Mobile Chrome\"",
    "test:mobile:safari": "playwright test tests/mobile/ --project=\"Mobile Safari\"",
    "test:mobile:tablet": "playwright test tests/mobile/ --project=\"Tablet\"",
    "test:phase1": "playwright test tests/security/phase1-security.test.ts",
    "test:ci": "playwright test tests/critical/ --reporter=json --reporter=html",
    "test:install": "playwright install",
    "test:report": "playwright show-report",
    "test:quick": "bash scripts/quick-test.sh",
    "test:db:staging": "node scripts/test-database.js staging",
    "test:api:staging": "node scripts/test-api.js staging",
    "type-check": "tsc --noEmit --skipLibCheck",
    "db:status": "node scripts/check-db-status.js",
    "db:seed": "node scripts/seed-database.js",
    "db:seed:local": "node scripts/seed-database.js local",
    "db:seed:staging": "node scripts/seed-database.js staging",
    "supabase:start": "node scripts/setup-local-supabase.js",
    "supabase:stop": "docker-compose down",
    "supabase:logs": "docker-compose logs -f",
    "supabase:reset": "docker-compose down -v && docker-compose up -d && node scripts/seed-database.js local",
    "backup:run": "node scripts/backup/simple-backup.js run",
    "backup:force": "node scripts/backup/simple-backup.js force",
    "backup:list": "node scripts/backup/simple-backup.js list",
    "backup:status": "node scripts/backup/simple-backup.js status",
    "backup:daily": "node scripts/backup/daily-backup.js run",
    "backup:daily:setup": "node scripts/backup/daily-backup.js setup",
    "backup:daily:status": "node scripts/backup/daily-backup.js status",
    "backup:daily:test": "node scripts/backup/daily-backup.js test",
    "backup:cloud": "node scripts/backup/google-drive-upload.js run",
    "backup:cloud:setup": "node scripts/backup/google-drive-upload.js run",
    "backup:cloud:list": "node scripts/backup/google-drive-upload.js list",
    "security:audit": "node scripts/security/audit-database.js run",
    "security:check-rls": "node scripts/security/audit-database.js run",
    "security:enable-rls": "cat scripts/security/enable-rls.sql",
    "security:implement-rls": "node scripts/security/implement-rls.js implement",
    "security:verify-rls": "node scripts/security/implement-rls.js verify",
    "security:test": "node scripts/security/test-rls.js run",
    "security:test-simple": "node scripts/security/simple-security-test.js run",
    "performance:audit": "node scripts/performance/analyze-performance.js audit",
    "performance:optimize": "node scripts/performance/analyze-performance.js optimize",
    "performance:run": "node scripts/performance/analyze-performance.js run",
    "performance:test": "node scripts/performance/basic-performance-test.js run",
    "performance:test-advanced": "node scripts/performance/run-advanced-test-fixed.js run",
    "performance:sql-audit": "cat scripts/performance/audit.sql",
    "performance:sql-optimize": "cat scripts/performance/optimize.sql",
    "security:test-user": "node scripts/security/test-rls.js user",
    "security:test-public": "node scripts/security/test-rls.js public",
    "security:test-admin": "node scripts/security/test-rls.js admin",
    "security:instructions": "node scripts/security/simple-security-check.js instructions"
  },
  "dependencies": {
    "@react-google-maps/api": "^2.20.8",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.1",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/nodemailer": "^7.0.5",
    "@upstash/redis": "^1.36.1",
    "autoprefixer": "^10.4.14",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.12",
    "postcss": "^8.4.24",
    "razorpay": "^2.9.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-places-autocomplete": "^7.3.0",
    "recharts": "^3.7.0",
    "redis": "^5.10.0",
    "resend": "^6.7.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.30",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-places-autocomplete": "^7.2.14",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jest": "^30.2.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6"
  }
}
